{% block body %}
<div class="page-content-inner">
  <div class="row">
    <div class="col-md-12">

      <div class="portlet light ">
        <div class="portlet-title">
          <div class="caption font-dark">
            <i class="icon-map font-dark"></i>
            <span class="caption-subject bold uppercase"> {{ title|raw }} </span>
            <br/>
            <span class="caption-subject"> {{ subtitle|raw }} </span>          
          </div>
        </div>
        <div>
            {% include table_template %}        
        </div>
      </div>
    </div>
  </div>
</div>
{% endblock %}

