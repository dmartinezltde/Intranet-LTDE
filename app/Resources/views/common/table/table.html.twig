<table id="datatable_1" class="table table-striped table-bordered table-hover order-column" style="border: 1px solid black;" border="1px">
  <thead>
    <tr>
      {% for column in columns %}
        <th>{{ column }}</th>
      {% endfor %}
    </tr>
  </thead>
  <tbody>
    {% for row in table.getPreformattedData %}
      <tr>
      {% for cell in row %}
        {% if cell.isBool %}
          <td colspan="{{ cell.getColSpan }}" rowspan="{{ cell.getRowSpan }}" class="text-center {{ cell.getClassesStr }}">
            {# {% include 'data_types/_boolean.html.twig' with { 'value': cell.getValue } %} #}
            {{ linkProvider.createLink(cell.getValue)|raw }}
          </td>
        {% elseif cell.isView %}
          <td colspan="{{ cell.getColSpan }}" rowspan="{{ cell.getRowSpan }}" class="{{ cell.getClassesStr }}" >
            {# {{ render(controller(cell.getValue.getController, cell.getValue.getArguments)) }} #}
            {{ linkProvider.createLink(cell.getValue)|raw }}
          </td>
        {% else %}
          <td colspan="{{ cell.getColSpan }}" rowspan="{{ cell.getRowSpan }}" class="{{ cell.getClassesStr }}">
            {# {{ cell.getValue|raw }} #}
            {{ linkProvider.createLink(cell.getValue)|raw }}
          </td>
        {% endif %}
      {% endfor %}
      </tr>
    {% endfor %}
  </tbody>
</table>
